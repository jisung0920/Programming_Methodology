package BankPanelUI;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTextField;

public class startBanking extends JPanel implements BankViewSetting{
	JButton createBtn;
	JTextField bankName;
	public startBanking() {
		// TODO Auto-generated constructor stub
		setLayout(null);

		bankName = new JTextField("은행이름");
		createBtn = new JButton("생성");

		viewSetting();
		
		this.add(bankName);
		
		
		
		this.add(createBtn);

	}
	@Override
	public void viewSetting() {
		bankName.setSize(70,20);
		bankName.setLocation(400,400);
		
		createBtn.setSize(70,20);
		createBtn.setLocation(10,10);
		// TODO Auto-generated method stub
		
	}
	@Override
	public void valueSetting() {
		// TODO Auto-generated method stub
		
	}

}
