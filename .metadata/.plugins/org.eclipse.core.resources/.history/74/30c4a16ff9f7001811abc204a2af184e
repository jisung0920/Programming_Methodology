package BankPanelUI;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTextField;

public class startBanking extends JPanel {
	JButton createBtn;
	JTextField bankName;
	public startBanking() {
		// TODO Auto-generated constructor stub
		setLayout(null);

		bankName = new JTextField("은행이름");
		createBtn = new JButton("생성");
		
		System.out.println("start");

		bankName.setSize(70,20);
		bankName.setLocation(400,400);
		this.add(bankName);
		
		
		createBtn.setSize(70,20);
		createBtn.setLocation(10,10);
		this.add(createBtn);

	}

}
