package BankPanelUI;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTextField;;

public class startScreen extends JPanel {

	JButton createBtn;
	JTextField bankName;
	public startScreen() {
		setLayout(null);
		createBtn = new JButton("생성");
		bankName = new JTextField("은행이름");
		this.add(createBtn);
		this.add(bankName);
	}
}
