package BankPanelUI;

import javax.swing.JButton;
import javax.swing.JPanel;
import javax.swing.JTextField;;

public class loginScreen extends JPanel {

	JButton cLoginBtn,mLoginBtn;
	JTextField customerId,customerPw,managerId,managerPw;
	public loginScreen() {
		setLayout(null);
		cLoginBtn = new JButton();
		mLoginBtn = new JButton();
		
		customerId = new JTextField();
		customerPw = new JTextField();
		managerId = new JTextField();
		managerPw = new JTextField();
		this.add(customerId);
		this.add(customerPw);
		this.add(cLoginBtn);
		this.add(managerId);
		this.add(managerPw);
		this.add(mLoginBtn);
	}
}
